    "extend": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.0",
    "gulp-clean-css": "^2.0.7",
    "gulp-concat": "^2.6.0",
    "gulp-concat-css": "^2.2.0",
    "gulp-filter": "^4.0.0",
    "gulp-inject": "^4.0.0",
    "gulp-inline-ng2-template": "^1.1.2",
    "gulp-load-plugins": "^1.2.2",
    "gulp-plumber": "~1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-protractor": "^2.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-shell": "~0.5.2",
    "gulp-sourcemaps": "2.0.0-alpha",
    "gulp-template": "^4.0.0",
    "gulp-tslint": "^5.0.0",
    "gulp-typedoc": "^2.0.0",
    "gulp-typescript": "~2.13.4",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.5",
    "is-ci": "^1.0.8",
    "isparta": "^4.0.0",
    "isstream": "^0.1.2",
    "jasmine-core": "~2.4.1",
    "jasmine-reporters": "^2.2.0",
    "jasmine-spec-reporter": "^2.4.0",
    "jspm": "^0.17.0-beta.29",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "~1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-uiuxengineering-jspm": "^3.0.2",
    "lodash": "^4.15.0",
    "merge-stream": "^1.0.0",
    "open": "0.0.5",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-reporter": "^1.3.3",
    "protractor": "^3.3.0",
    "protractor-jasmine2-screenshot-reporter": "^0.3.2",
    "remap-istanbul": "^0.6.4",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.1.5",
    "semver": "^5.1.0",
    "serve-index": "^1.8.0",
    "serve-static": "^1.10.2",
    "slash": "~1.0.0",
    "stream-series": "^0.1.1",
    "stylelint": "^6.3.3",
    "stylelint-config-standard": "^6.0.0",
    "systemjs-istanbul-hook": "^0.1.0",
    "tildify": "^1.2.0",
    "tiny-lr": "^0.2.1",
    "traceur": "^0.0.91",
    "ts-node": "^0.7.2",
    "tslint": "^3.11.0",
    "tslint-stylish": "2.1.0-beta",
    "typedoc": "^0.4.4",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "walk": "^2.3.9",
    "yargs": "^4.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jspm": {
    "directories": {
      "packages": "src/browser/jspm_packages"
    },
    "configFiles": {
      "jspm": "src/browser/jspm.config.js",
      "jspm:dev": "jspm.dev.js",
      "jspm:browser": "jspm.browser.js",
      "jspm:node": "jspm.node.js"
    },
    "dependencies": {
      "@angular/forms": "npm:@angular/forms@^2.1.0",
      "@angular/http": "npm:@angular/http@^2.1.0",
      "@angular/platform-browser-dynamic": "npm:@angular/platform-browser-dynamic@^2.1.0",
      "@angular/router": "npm:@angular/router@^3.1.0",
      "UIUXEngineering/plugin-css-string": "github:UIUXEngineering/plugin-css-string@master",
      "core-js": "npm:core-js@^2.4.1",
      "es6-shim": "github:es-shims/es6-shim@^0.35.1",
      "normalize.css": "github:necolas/normalize.css@^5.0.0",
      "reflect-metadata": "npm:reflect-metadata@^0.1.8",
      "systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.6.0",
      "text": "github:systemjs/plugin-text@^0.0.9"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.16",
      "ts": "github:frankwallis/plugin-typescript@^5.2.7"
    },
    "peerDependencies": {
      "@angular/common": "npm:@angular/common@2.1.0",
      "@angular/compiler": "npm:@angular/compiler@2.1.0",
      "@angular/core": "npm:@angular/core@2.1.0",
      "@angular/platform-browser": "npm:@angular/platform-browser@2.1.0",
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "rxjs": "npm:rxjs@5.0.0-beta.12",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "timers": "github:jspm/nodelibs-timers@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha",
      "zone.js": "npm:zone.js@^0.6.21"
    },
    "overrides": {
      "github:socketio/socket.io-client@1.5.0": {
        "main": "socket.io.js"
      },
      "npm:@angular/common@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/common": ".",
          "./testing": "./bundles/common-testing.umd.js"
        }
      },
      "npm:@angular/compiler@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/compiler": ".",
          "./testing": "./bundles/compiler-testing.umd.js"
        }
      },
      "npm:@angular/core@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/core": ".",
          "./testing": "./bundles/core-testing.umd.js"
        }
      },
      "npm:@angular/http@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/http": ".",
          "./testing": "./bundles/http-testing.umd.js"
        }
      },
      "npm:@angular/platform-browser-dynamic@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/platform-browser-dynamic": ".",
          "./testing": "./bundles/platform-browser-dynamic-testing.umd.js"
        }
      },
      "npm:@angular/platform-browser@2.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/platform-browser": ".",
          "./testing": "./bundles/platform-browser-testing.umd.js"
        }
      },
      "npm:@angular/router@3.1.0": {
        "meta": {
          "src/*.js": {
            "deps": [
              "reflect-metadata",
              "zone.js"
            ]
          }
        },
        "map": {
          "@angular/router": ".",
          "./testing": "./bundles/router-testing.umd.js"
        }
      },
      "npm:debug@2.2.0": {
        "main": "browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./browser.js": {
            "node": "./node.js"
          },
          "./node.js": {
            "browser": "./browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:ms@0.7.1": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:reflect-metadata@0.1.8": {
        "jspmNodeConversion": false,
        "map": {
          "crypto": {
            "node": "@node/crypto",
            "default": "@empty"
          }
        }
      },
      "npm:typescript@2.0.3": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  }
}
